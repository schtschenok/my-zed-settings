[
    {
        "bindings": {
            // JetBrains
            // "ctrl-alt-s": "zed::OpenSettings",
            // "ctrl-alt-k": "zed::OpenKeymap"
        }
    },
    {
        "context": "Workspace",
        "bindings": {
            // JetBrains
            // "ctrl-shift-n": "file_finder::Toggle",
            "ctrl-shift-a": "command_palette::Toggle",
            "ctrl-alt-shift-n": "project_symbols::Toggle",
            "ctrl-e": "tab_switcher::Toggle",
            // My stuff
            "shift shift": "project_symbols::Toggle",
            "alt-t": "terminal_panel::ToggleFocus",
            "ctrl-r": "task::Rerun",
            "ctrl-t": "file_finder::Toggle",
            "ctrl-1": ["pane::ActivateItem", 0],
            "ctrl-2": ["pane::ActivateItem", 1],
            "ctrl-3": ["pane::ActivateItem", 2],
            "ctrl-4": ["pane::ActivateItem", 3],
            "ctrl-5": ["pane::ActivateItem", 4],
            "ctrl-6": ["pane::ActivateItem", 5],
            "ctrl-7": ["pane::ActivateItem", 6],
            "ctrl-8": ["pane::ActivateItem", 7],
            "ctrl-9": ["pane::ActivateItem", 8],
            "ctrl-0": "pane::ActivateLastItem",
            "alt-1": "workspace::ToggleLeftDock",
            "alt-2": "project_panel::ToggleFocus",
            "alt-3": "outline_panel::ToggleFocus",
            "alt-4": "workspace::ToggleRightDock",
            "alt-5": null,
            "alt-6": null,
            "alt-7": null,
            "alt-8": null,
            "alt-9": null,
            "alt-0": null,
            "ctrl-alt-left": "workspace::ActivatePaneLeft",
            "ctrl-alt-right": "workspace::ActivatePaneRight",
            "ctrl-alt-j": "workspace::ActivatePaneLeft",
            "ctrl-alt-k": "workspace::ActivatePaneRight"
        }
    },
    {
        "context": "Pane",
        "bindings": {
            // JetBrains
            "ctrl-alt-left": "pane::GoBack",
            "ctrl-alt-right": "pane::GoForward",
            // My stuff
            "alt-1": "workspace::ToggleLeftDock",
            "alt-2": "project_panel::ToggleFocus",
            "alt-3": "outline_panel::ToggleFocus",
            "alt-4": "workspace::ToggleRightDock",
            "alt-5": null,
            "alt-6": null,
            "alt-7": null,
            "alt-8": null,
            "alt-9": null,
            "alt-0": null
        }
    },
    {
        "context": "Editor",
        "bindings": {
            // JetBrains
            "ctrl-shift-j": "editor::JoinLines",
            "ctrl-d": "editor::DuplicateSelection",
            "ctrl-y": "editor::DeleteLine",
            "ctrl-m": "editor::ScrollCursorCenter",
            "ctrl-pagedown": "editor::MovePageDown",
            "ctrl-pageup": "editor::MovePageUp",
            "ctrl-alt-enter": "editor::NewlineAbove",
            "alt-shift-g": "editor::SplitSelectionIntoLines",
            "alt-j": ["editor::SelectNext", { "replace_newest": false }],
            "alt-shift-j": [
                "editor::SelectPrevious",
                { "replace_newest": false }
            ],
            "ctrl-/": ["editor::ToggleComments", { "advance_downwards": true }],
            "shift-alt-up": "editor::MoveLineUp",
            "shift-alt-down": "editor::MoveLineDown",
            "ctrl-alt-l": "editor::Format",
            "shift-f6": "editor::Rename",
            // "ctrl-alt-left": "pane::GoBack",
            // "ctrl-alt-right": "pane::GoForward",
            "alt-f7": "editor::FindAllReferences",
            "ctrl-alt-f7": "editor::FindAllReferences",
            "ctrl-b": "editor::GoToDefinition",
            "ctrl-alt-b": "editor::GoToDefinitionSplit",
            "ctrl-shift-b": "editor::GoToTypeDefinition",
            "ctrl-alt-shift-b": "editor::GoToTypeDefinitionSplit",
            "f2": "editor::GoToDiagnostic",
            "shift-f2": "editor::GoToPreviousDiagnostic",
            "ctrl-alt-shift-down": "editor::GoToHunk",
            "ctrl-alt-shift-up": "editor::GoToPreviousHunk",
            "ctrl-alt-z": "git::Restore",
            "ctrl-home": "editor::MoveToBeginning",
            "ctrl-end": "editor::MoveToEnd",
            "ctrl-shift-home": "editor::SelectToBeginning",
            "ctrl-shift-end": "editor::SelectToEnd",
            // My stuff
            "ctrl-shift-d": "diagnostics::Deploy",
            "ctrl-alt-left": "workspace::ActivatePaneLeft",
            "ctrl-alt-right": "workspace::ActivatePaneRight",
            "ctrl-0": "pane::ActivateLastItem"
        }
    },
    {
        "context": "Editor && mode == full",
        "bindings": {
            // JetBrains
            "ctrl-f12": "outline::Toggle",
            "alt-7": "outline::Toggle",
            // "ctrl-shift-n": "file_finder::Toggle",
            "ctrl-g": "go_to_line::Toggle",
            "alt-enter": "editor::ToggleCodeActions"
        }
    },
    {
        "context": "Editor && renaming",
        "bindings": {
            // My stuff
            "enter": "editor::ConfirmRename"
        }
    },
    {
        "context": "Editor && showing_completions",
        "bindings": {
            // My stuff
            "enter": "editor::ConfirmCompletion",
            "tab": "editor::ComposeCompletion"
        }
    },
    {
        "context": "Editor && multibuffer",
        "bindings": {
            // My stuff
            "ctrl-alt-enter": "editor::OpenExcerpts"
        }
    },
    {
        "context": "ProjectPanel",
        "bindings": {
            // JetBrains
            "enter": "project_panel::Open",
            "backspace": ["project_panel::Trash", { "skip_prompt": false }],
            "delete": ["project_panel::Trash", { "skip_prompt": false }],
            "shift-delete": ["project_panel::Delete", { "skip_prompt": false }],
            "shift-f6": "project_panel::Rename"
        }
    },
    {
        "context": "BufferSearchBar",
        "bindings": {
            // My stuff
            "enter": "search::SelectNextMatch",
            "shift-enter": "search::SelectPreviousMatch",
            "ctrl-r": "search::ToggleReplace"
        }
    },
    {
        "context": "Terminal",
        "bindings": {
            // My stuff
            "ctrl-shift-t": "workspace::NewTerminal",
            "ctrl-w": "pane::CloseActiveItem"
        }
    }
]
